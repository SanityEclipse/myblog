<h1> This is the 'Update Entry' page </h1>


<h1>Update Entry</h1>

{{#each entry}}

<form action="/updateEntry/{{id}}" method="POST">
  <input type="text" value="{{this.title}}" name='title'></input>
  <input type="text" value="{{this.content}}" name='content'></input>
  <input type="text" value="{{this.date}}" name='date'></input>
  <input type="submit" value="Update Entry"></input>
</form>

{{/each}}
